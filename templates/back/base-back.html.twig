                                    <td>{{ p.tauxPromotion }}%</td>
                                    <td>{{ p.description }}</td>
                                    <td><a style=color:red href="{{ path('deleteP',{'id':p.id}) }}">supprimer</a></td>
                                    <td><a style=color:#00c94a href="{{ path('updateP',{'id':p.id}) }}">update</a></td>
                                </tr>
                            {% endfor %}

                            </tbody>
                        </table>
